    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Test EfficientFormer Model",
            "type": "debugpy",
            "request": "launch",
            "args" : ["--num_envs", "10"],
            "program": "${workspaceFolder}/test_efficient_former.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Python Debugger: Test GoT Model",
            "type": "debugpy",
            "request": "launch",
            "args" : ["--num_envs", "10"],
            "program": "${workspaceFolder}/test_got.py",
            "console": "integratedTerminal",
        },
        {
            "name": "Python Debugger: Test Compilable GoT Model",
            "type": "debugpy",
            "request": "launch",
            "args" : ["--num_envs", "10"],
            "program": "${workspaceFolder}/test_compilable_got.py",
            "console": "integratedTerminal",
        }
    ]
}